{
  "name": "server",
  "version": "1.0.0",
  "description": "Servidor Express com TypeScript e decorators",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate": "npm run build && npx sequelize-cli db:migrate",
    "db:migrate:undo": "npm run build && npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npm run build && npx sequelize-cli db:migrate:undo:all",
    "db:seed:all": "npm run build && npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "npm run build && npx sequelize-cli db:seed:undo:all",
    "db:init": "npm run build && node dist/infra/database/init-db.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7",
    "umzug": "^3.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/umzug": "^2.3.9",
    "@types/uuid": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
